enum Error {
    Ok = 0;
    InvalidRequest = 1;
}

// this is a comment
sequence MoveToEntry {
    id: u8; // this is another comment
    dest: f32;
    speed: f32;
}

sequence MoveTo {
    entries: [MoveToEntry];
}

sequence Init {
    expected_fw_version: u32;
}

sequence Request {
    request_id: u32;
    payload: oneof {
        init: Init;
        move_to: MoveTo;
    };
}