enum RobotJoint {
    j0 = 0;
    j1 = 1;
    j2 = 2;
    j3 = 3;
    j4 = 4;
    j5 = 5;
}

enum BigBoy {
    only_option = 999999;
}

sequence Request {
    id: u32;
    payload: oneof {
        init: Init;
        moveTo: MoveTo;
        testOneOf: oneof {
            moveToEntry: MoveToEntry;
            stringTest: StringTest;
            bigBoy: BigBoy;
        };
    };
}

sequence Init {
    expected_firmware: u32;
}

sequence MoveTo {
    joints: [MoveToEntry];
}

sequence MoveToEntry {
    joint: RobotJoint;
    angle: f32;
    speed: f32;
}

sequence StringTest {
    fields: oneof {
        test: string;
        string: i64;
    };
}